<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id = "dog" class = "com.classwork.Dog">
	<property name="color" value = "brown"></property>
	<property name="breed" value = "husky"></property>
	<property name="year" value = "2016"></property>
	<property name="food" ref = "food1"></property>
	</bean>
	
	<bean id = "food1" class = "com.classwork.Food">
	<property name="type" value = "pet feed"></property>
	<property name="quantity" value = "10"></property>
	
	
	</bean>
		
	
</beans>