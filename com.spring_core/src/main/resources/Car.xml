<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="car" class="spring_core.Car">
		<property name="color" value="red"></property>
		<property name="brand" value="bmw"></property>
		<property name="price" value="2000"></property>
	</bean>
	
	<bean id="laptop" class="com.assigment.Laptop">
	    <property name="price" value="900"></property>
		<property name="brand" value="dell"></property>
		<property name="made" value="usa"></property>
		<property name="color" value="silver"></property>
		
	</bean>
</beans>